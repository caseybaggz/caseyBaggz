{
  "name": "caseybaggz",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://caseybaggz.com",
  "devDependencies": {
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "eslint-plugin-prettier": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts": "^2.0.4"
  },
  "dependencies": {
    "flow-bin": "^0.83.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "normalize.css": "^8.0.0",
    "prettier": "^1.14.3",
    "raven-js": "^3.27.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
